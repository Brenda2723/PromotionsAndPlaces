<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ConceptoCupon">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/fondocupon"
        tools:layout_editor_absoluteX="-16dp">

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="265dp"
            android:layout_height="183dp"
            android:layout_marginTop="64dp"
            android:src="@drawable/promotionsicono"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"></ImageView>

        <TextView
            android:id="@+id/textView3"
            android:layout_width="322dp"
            android:layout_height="128dp"
            android:layout_marginTop="65dp"
            android:gravity="center"
            android:text="@string/txt_pideEmpleado"
            android:textColor="#0E0D0D"
            android:textSize="25sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.494"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView6" />

        <Button
            android:id="@+id/btn_regresar2"
            android:layout_width="259dp"
            android:layout_height="56dp"
            android:layout_marginBottom="100dp"
            android:background="@drawable/btn_fondo_color"
            android:elevation="5dp"
            android:onClick="PerfilFragment"
            android:text="@string/btn_regresar"
            android:textColor="#fff"
            android:textSize="16sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.519"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView3"
            app:layout_constraintVertical_bias="1.0"></Button>
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>