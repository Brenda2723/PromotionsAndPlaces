<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ConceptoCupon">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/fondocupon"
        tools:layout_editor_absoluteX="324dp"
        tools:layout_editor_absoluteY="-38dp">

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="265dp"
            android:layout_height="183dp"
            android:src="@drawable/promotionsicono"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.165"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.377"></ImageView>

        <TextView
            android:id="@+id/textView3"
            android:layout_width="322dp"
            android:layout_height="128dp"
            android:layout_marginEnd="32dp"
            android:gravity="center"
            android:text="@string/txt_pideEmpleado"
            android:textColor="#151414"
            android:textSize="25sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.714"
            app:layout_constraintStart_toEndOf="@+id/imageView6"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.416"
            tools:ignore="MissingConstraints" />

        <Button
            android:id="@+id/btn_regresar2"
            android:layout_width="259dp"
            android:layout_height="56dp"
            android:layout_marginBottom="16dp"
            android:background="@drawable/btn_fondo_color"
            android:elevation="5dp"
            android:onClick="PerfilFragment"
            android:text="@string/btn_regresar"
            android:textColor="#fff"
            android:textSize="16sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            tools:ignore="MissingConstraints"></Button>
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>